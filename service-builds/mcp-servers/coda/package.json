{
  "name": "coda-mcp-server",
  "version": "2.0.0",
  "description": "Coda MCP server with Stytch OAuth 2.1 authentication (Phase 2)",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:server && npm run build:auth",
    "build:server": "tsc",
    "build:auth": "vite build",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:auth": "vite dev",
    "lint": "eslint --ext .ts src --ext .ts,.tsx authorization-ui/src",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@stytch/react": "^19.15.2",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "stytch": "^12.43.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0",
    "vite": "^5.4.10"
  },
  "keywords": [
    "mcp",
    "coda",
    "stytch",
    "oauth2.1",
    "authentication"
  ],
  "author": "BestViable",
  "license": "MIT"
}
